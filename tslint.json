{
  "defaultSeverity": "error",
  "extends": [
  "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "indent": [true, "tabs", 1],
    "quotemark": [true, "single", "avoid-escape"],
    "semicolon": [true, "never"],
    "trailing-comma": [true, {"multiline": "never", "singleline": "never"}],
    "no-console": false,
    "arrow-parens": false,
    "object-literal-sort-keys": false,
    "max-line-length": false
  },
  "rulesDirectory": []
}